library ieee;
use ieee.std_logic_1164.all;

entity testbench is
end entity testbench;

architecture behaviour of testbench is
  component pipelined_mult is
  	port (A, X: in std_logic_vector(31 downto 0);
		clk, reset: in std_logic;
		P: out std_logic_vector(63 downto 0));
  end component;
  signal clk, reset: std_logic;
  signal A, X: std_logic_vector(31 downto 0);
  signal P: std_logic_vector(63 downto 0);
begin
  clk_process: process
  begin
	clk <= '0';
	wait for 5 ns;
 	clk <= '1';
	wait for 5 ns;
  end process;
  input_process: process
  begin
	A <= "00000000000000000000000000000001";
	X <= "00000000000000000000000000001111";
	wait for 50 ns;
	A <= "00000000000000000000000000000001";
	X <= "01111111111111111111111111111111";
	wait for 50 ns;
	A <= "00000000000000000000000000000001";
	X <= "11111111111111111111111111111111";
	wait for 50 ns;
	A <= "01111111111111111111111111111111";
	X <= "01111111111111111111111111111111";
	wait for 50 ns;
	A <= "11111111111111111111111111111111";
	X <= "01111111111111111111111111111111";
	wait for 50 ns;
	A <= "00000000000000000000000000001010";
	X <= "00000000000000000000000001100100";
	wait for 50 ns;
  end process;
  pm : pipelined_mult port map(A=>A, X=>X, clk=>clk, reset=>reset, P=>P);
end behaviour;
